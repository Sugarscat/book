{
  "index_name": "note",
  "start_urls": ["https://note.sugarscat.com/"],
  "stop_urls": ["/search"],
  "rateLimit": 8,
  "maxDepth": 10,
  "selectors": {
    "default": {
      "lvl1": ".content h1",
      "lvl2": ".content h2",
      "lvl3": ".content h3",
      "lvl4": ".content h4",
      "lvl5": ".content h5",
      "text": ".content p, .content li"
    }
  },
  "selectors_exclude": [
    "aside",
    ".page-footer",
    ".next-and-prev-link",
    ".table-of-contents"
  ],
  "js_render": true
}